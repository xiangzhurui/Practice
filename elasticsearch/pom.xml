        </dependency>
        <dependency>
            <groupId>org.elasticsearch.test</groupId>
            <artifactId>framework</artifactId>
        </dependency>
        <!-- /Elasticesearch 相关 -->
        <!-- 工具 -->
        <dependency>
            <groupId>com.google.code.gson</groupId>
